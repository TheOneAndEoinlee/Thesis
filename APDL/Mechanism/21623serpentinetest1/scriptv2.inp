ksel,s,kp,,7,kpo,kpnum
ksel,a,kp,,10,kpo,kpnum
ksel,a,kp,,30,kpo,kpnum
ksel,a,kp,,33,kpo,kpnum
ksel,a,kp,,34,kpo,kpnum
ksel,a,kp,,35,kpo,kpnum
ksel,a,kp,,43,kpo,kpnum
ksel,a,kp,,46,kpo,kpnum
nslk,s
cm,fixed_nodes,node


!select nodes to be input
