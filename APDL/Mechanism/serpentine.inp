!     k,0,(2*i+1)*conlength,flexlength/2,0	
!     k,0,(2*i+2)*conlength,flexlength/2,0
! *enddo
! k,0,(2*numseg+2)*conlength,0,0

*do, i,1,numseg
    k,0,i*conlength,0,0
    k,0,i*conlength,flexlength,0
*enddo
k,0,(numseg+1)*conlength,0,0
*get,input_kp,kp,0,num,maxd

l,1,2
