*do,i,1,numseg
    l,2*i,2*i+1
    l,2*i+0.5+toggle,2*i+2.5+toggle
    toggle = -toggle
*enddo


lsel,s,length,,flexlength
secnum,1
lesize,all,0,,10
lmesh,all
allsel

lsel,s,length,,conlength
secnum,2
lesize,all,0,,3
lmesh,all
allsel



/eshape, 1
/view, 1, 1, 1, 1
eplot

