! flexlength_vec(2) = 6.4e-3

! *do,k,1,nsteps
! flexlength = flexlength_vec(k)

! /prep7
! ALLSEL, ALL  ! Selects all entities
! CMDELE,id_fixed_nodes
! lclear, all  ! Clears all entities
! LDELE, ALL  ! Deletes all lines
! KDELE, ALL  ! Deletes all keypoints




!define material and element type
/prep7
et,1,beam188
keyopt,1,1,1

MP, EX, 1, 2.2e9 !Young's modulus
MP, PRXY, 1, 0.3 !Poisson's ratio
mptemp,1,0 !temperature

sectype,1,beam,rect
secdata,t,w
sectype,2,beam,rect
secdata,tc,w

!define geometry

k,0,0,0,0


! *do,i,1,numseg,
!     k,0,2*i*conlength,-flexlength/2,0
